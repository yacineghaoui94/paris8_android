<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    tools:context=".Yacine3"
    tools:showIn="@layout/Yacine3">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#4399ef"
        android:gravity="center"
        app:popupTheme="@style/AppTheme">


        <ImageButton
            android:id="@+id/app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:src="@drawable/aid" />

        <ImageButton
            android:id="@+id/appler"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:src="@drawable/aide" />

        <TextView
            android:id="@+id/textv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:text="10"
            android:textColor="#000007"
            android:textSize="24dp" />

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@android:drawable/btn_star" />

    </androidx.appcompat.widget.Toolbar>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/img1"
            android:layout_width="145dp"
            android:layout_height="92dp"
            android:layout_margin="1dp"
            android:src="@drawable/bboo" />

        <ImageButton
            android:id="@+id/img2"
            android:layout_width="145dp"
            android:layout_height="92dp"
            android:layout_margin="1dp"
            android:src="@android:drawable/ic_menu_delete" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/img3"
            android:layout_width="145dp"
            android:layout_height="92dp"
            android:layout_margin="1dp"
            android:src="@android:drawable/ic_menu_delete" />

        <ImageButton
            android:id="@+id/img4"
            android:layout_width="145dp"
            android:layout_height="92dp"
            android:layout_margin="1dp"

            android:src="@android:drawable/ic_menu_delete" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#ffcccc"
        android:gravity="center">

        <TextView
            android:id="@+id/ite1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#fff"
            android:hint="1"
            android:padding="12dp"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ite2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#fff"
            android:hint="2"
            android:padding="12dp"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ite3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#fff"
            android:hint="3"
            android:padding="12dp"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ite4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#fff"
            android:hint="4"
            android:padding="12dp"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ite5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#fff"
            android:hint="5"
            android:padding="12dp"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/bot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            android:src="@drawable/trouv" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/bot1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="A" />

        <TextView
            android:id="@+id/bot2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="I" />

        <TextView
            android:id="@+id/bot3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="C" />

        <TextView
            android:id="@+id/bot4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="U" />

        <TextView
            android:id="@+id/bot5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="T" />

        <TextView
            android:id="@+id/bot6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="F" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:gravity="center">

        <TextView
            android:id="@+id/bot7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="S" />

        <TextView
            android:id="@+id/bot8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="A" />

        <TextView
            android:id="@+id/bot9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="L" />

        <TextView
            android:id="@+id/bot10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="O" />

        <TextView
            android:id="@+id/bot11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="Z" />

        <TextView
            android:id="@+id/bot12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:background="#00a0ff"
            android:padding="10dp"
            android:text="N" />

    </LinearLayout>
</LinearLayout>
